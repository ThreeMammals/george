{
    "name": "three-mammals/pushy",
    "description": "A plugin to make Wordpress headless.",
    "license": "MIT",
    "type": "wordpress-plugin",
    "autoload": {
        "classmap": [
            "src/"
        ],
        "psr-0": {
            "Pushy":        "src/"
        }
    },
    "config": {
        "vendor-dir": "vendor/"
    },
    "require-dev": {
        "phpunit/phpunit": "^7",
        "10up/wp_mock": "0.4.0"
    },
    "require": {
        "composer/installers": "~1.0",
        "aws/aws-sdk-php-resources": "^0.3.0",
        "ramsey/uuid": "^3.8",
        "php-amqplib/php-amqplib": "^2.9",
        "ext-sockets": "*",
        "ext-bcmath": "*"
    }
}
